<h1>Example : Image Quality</h1>
<!--<%= session[:test_set] %>
<%= session[:subtask] %>
<%= session[:first] %>-->

<div align="center">
  <% if @state == 'semanticRecognition' %>
      <%= render partial: 'layouts/semanticRecognition' %>
  <% elsif @state == 'distortion_visible_state' %>
      <%= render partial: 'layouts/distortionVisible' %>
  <% elsif @state == 'describe_details_state' %>
      <% if @type == 'indoor' %>
          <%= render partial: 'layouts/indoorDetails' %>
      <% elsif @type == 'outdoor_natural' %>
          <%= render partial: 'layouts/outdoorNaturalDetails' %>
      <% elsif @type == 'outdoor_man_made' %>
          <%= render partial: 'layouts/outdoorManMadeDetails' %>
      <% end %>
  <% end %>
</div>

<div class="buttons">
  <%= link_to "Next", question_path, class: "btn btn-large btn-primary" %>
</div>
